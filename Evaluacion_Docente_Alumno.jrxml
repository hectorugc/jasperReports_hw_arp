	<subDataset name="New Dataset 1" uuid="7583fcec-8fe5-4fe2-a346-8164d1d9c855">
		<parameter name="id_institucion" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_cuatrimestre" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_anio" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_alumno" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_profesor" class="java.lang.Long"/>
		<queryString>
			<![CDATA[Select distinct ccpeda.descripcion_pregunta,creda.respuesta from operation.evaluacion_docente_alumno_detalle oedad
full outer join catalog.cat_preguntas_evaluacion_docente_alumno ccpeda on oedad.pregunta = ccpeda.id_pregunta and oedad.id_cuatrimestre = ccpeda.id_cuatrimestre and oedad.id_anio = ccpeda.id_anio and oedad.id_institucion = ccpeda.id_institucion
full outer join catalog.cat_respuestas_evaluacion_docente_alumno creda on oedad.respuesta = creda.id_respuesta and oedad.id_institucion = creda.id_institucion and oedad.pregunta = creda.id_pregunta and oedad.id_cuatrimestre = creda.id_cuatrimestre and oedad.id_anio = creda.id_anio
where oedad.id_institucion = $P{id_institucion} and
	 oedad.id_anio = $P{id_anio} and
	 oedad.id_cuatrimestre = $P{id_cuatrimestre} and
	 oedad.id_alumno = $P{id_alumno} and
	oedad.id_profesor = $P{id_profesor}]]>
		</queryString>
		<field name="descripcion_pregunta" class="java.lang.String"/>
		<field name="respuesta" class="java.lang.String"/>
		<group name="descripcion_pregunta">
			<groupExpression><![CDATA[$F{descripcion_pregunta}]]></groupExpression>
		</group>
		<group name="respuesta">
			<groupExpression><![CDATA[$F{respuesta}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="id_institucion" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_anio" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_cuatrimestre" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_alumno" class="java.lang.String"/>
	<parameter name="id_profesor" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select  concat_ws(' ',al.nombre,al.app,al.apm) alumno ,concat_ws(' ',
prof.nombre,prof.app,prof.apm) profesor,
oedad.id_anio, cuati.descripcion
--cpeda.descripcion_pregunta,
--creda.respuesta
from operation.evaluacion_docente_alumno_detalle oedad
full outer join catalog.cat_preguntas_evaluacion_docente_alumno cpeda  on oedad.pregunta = cpeda.id_pregunta
full outer join catalog.cat_respuestas_evaluacion_docente_alumno creda on oedad.respuesta = creda.id_pregunta
full outer join entity.alumnos al on oedad.id_alumno = al.id_alumno
full outer join entity.profesores prof on oedad.id_profesor = prof.id_profesor
full outer join catalog.cat_cuatrimestre cuati on oedad.id_cuatrimestre = cuati.id_cuatrimestre
where
	oedad.id_institucion=1 and
	oedad.id_institucion=$P{id_institucion} and
    oedad.id_anio=$P{id_anio} and
    oedad.id_cuatrimestre=$P{id_cuatrimestre} and
    oedad.id_profesor=$P{id_profesor} and
	oedad.id_alumno=$P{id_alumno}
limit 1]]>
	</queryString>
	<field name="alumno" class="java.lang.String"/>
	<field name="profesor" class="java.lang.String"/>
	<field name="id_anio" class="java.lang.Long"/>
	<field name="descripcion" class="java.lang.String"/>
